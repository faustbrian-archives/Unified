{
    "name": "faustbrian/unified",
    "description": "Unified provides a common interface for building API Clients",
    "keywords": ["laravel", "framework", "api", "unified", "Brian Faust", "faustbrian"],
    "license": "MIT",
    "authors": [{
        "name": "Brian Faust",
        "email": "hello@brianfaust.de",
        "homepage": "https://brianfaust.de"
    }],
    "require": {
        "php": "^7.0",
        "illuminate/support": "5.3.* || 5.4.* || 5.5.*",
        "guzzlehttp/guzzle": "^6.2",
        "guzzlehttp/oauth-subscriber": "^0.3.0",
        "nesbot/carbon": "^1.21",
        "faustbrian/payload": "^1.0",
        "league/container": "^2.2",
        "somoza/oauth2-client-middleware": "^0.2.5",
        "league/uri": "^4.2",
        "psr/http-message": "^1.0",
        "psr/cache": "^1.0",
        "php-http/httplug": "^1.1",
        "php-http/discovery": "^1.1",
        "php-http/client-common": "^1.3",
        "php-http/cache-plugin": "^1.2",
        "php-http/guzzle6-adapter": "^1.1",
        "php-http/multipart-stream-builder": "^0.1.2",
        "netresearch/jsonmapper": "^1.0"
    },
    "require-dev": {
        "graham-campbell/testbench": "^3.3",
        "mockery/mockery": "^0.9",
        "phpunit/phpunit": "^6.1",
        "symfony/var-dumper": "^3.3",
        "symfony/debug": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "BrianFaust\\Unified\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BrianFaust\\Tests\\Unified\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
